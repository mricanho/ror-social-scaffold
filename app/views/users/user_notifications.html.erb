<h2>Notifications</h2>

<br>
<%= zero_notifications(@users) %>
<br>
<section class="users-section">
  <ul class="users-list">
  <% @users.each do |user| %>
    <li>         
      <% friendship = verify_friendship(user) %>
      <%= friends_notifications(friendship, user) %>
    </li>
    <br>
  <% end %>
  </ul>
</section>
